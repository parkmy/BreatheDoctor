@synthesize sessionDoctorID;
@synthesize urlPersonalGuide;
@synthesize currentPatientID;
@synthesize DBName;
@synthesize isLogin;
@synthesize netWorkOK;

static CDGloablVar *gloablVarSingleton = nil;

+ (CDGloablVar *) sharedGloablVar
{
